[*.cs]

# CS1998: 异步方法缺少 "await" 运算符，将以同步方式运行
dotnet_diagnostic.CS1998.severity = suggestion
[*.cs]

# CS1998: 异步方法缺少 "await" 运算符，将以同步方式运行
dotnet_diagnostic.CS1998.severity = none
[*.cs]

# CS1998: 异步方法缺少 "await" 运算符，将以同步方式运行
dotnet_diagnostic.CS1998.severity = suggestion
[*.cs]

# CS1998: 异步方法缺少 "await" 运算符，将以同步方式运行
dotnet_diagnostic.CS1998.severity = suggestion

# Default severity for all analyzer diagnostics
dotnet_analyzer_diagnostic.severity = none

# Default severity for all analyzer diagnostics
dotnet_analyzer_diagnostic.severity = silent

# IDE0058: 永远不会使用表达式值
dotnet_diagnostic.IDE0058.severity = suggestion

# Default severity for analyzer diagnostics with category 'Style'
dotnet_analyzer_diagnostic.category-Style.severity = silent

# IDE0320: 使匿名函数成为静态函数
dotnet_diagnostic.IDE0320.severity = silent

# CS8600: 将 null 字面量或可能为 null 的值转换为非 null 类型。
dotnet_diagnostic.CS8600.severity = suggestion

# CS8602: 解引用可能出现空引用。
dotnet_diagnostic.CS8602.severity = suggestion

# CS8604: 引用类型参数可能为 null。
dotnet_diagnostic.CS8604.severity = silent

# CS8622: 参数类型中引用类型的为 Null 性与目标委托不匹配(可能是由于为 Null 性特性)。
dotnet_diagnostic.CS8622.severity = silent
